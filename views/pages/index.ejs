<!DOCTYPE html>
<html>
<head>
    <% include ../partials/header.ejs %>
</head>
<body>
<% include ../partials/nav.ejs %>
<table class="pure-table">
    <thead>
    <tr>
        <th>name</th>
        <th>douban score</th>
        <th>updated time</th>
    </tr>
    </thead>
    <tbody>
    <% rows.forEach(function(row){ %>
    <tr>
        <td><%= row.name %></td>
        <td><a href="<%= row.url %>"><%= row.name_cn %> [ <%= row.score %> ]</a></td>
        <td><%= row.updated %></td>
    </tr>
    <% }); %>
    </tbody>
</table>
</body>
</html>
